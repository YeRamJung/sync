<mvc:View controllerName="sync6.exercise2110.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form">
    <Page id="page2" title="{i18n>title}" showNavButton="true" navButtonPress=".onGoBack">
        <Text text="두번째 페이지" />
        <!--{detailModel>keyA}-->

        <VBox class="sapUiSmallMargin">
            <f:SimpleForm id="SimpleFormChange354" editable="true" layout="ResponsiveGridLayout" title="학생 상세정보" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
                <f:content>
                    <!--학생번호-->
                    <Label text="학생번호" />
                    <Text text="{detailModel>/no}" />

                    <!--학생이름-->
                    <Label text="학생이름" />
                    <Text text="{detailModel>/name}" />

                    <!--t성별-->
                    <Label text="성별" />
                    <Text text="{detailModel>/gender}" />

                    <!--생년월일-->
                    <Label text="생년월일" />
                    <Text text="{detailModel>/birthdate}" />
                </f:content>
            </f:SimpleForm>
        </VBox>
    </Page>
</mvc:View>
